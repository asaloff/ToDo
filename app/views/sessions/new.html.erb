<%= render 'shared/content_title', title: 'Login' %>

<%= form_tag login_path do %>
  <div class="form-group">
    <%= label_tag :username %>
    </br>
    <%= text_field_tag :username %>
  </div>
  <div class="form-group">
    <%= label_tag :password %>
    </br>
    <%= password_field_tag :password %>
  </div>
  <%= submit_tag 'Login', class: 'btn btn-small btn-success' %>
<% end %>

<small id="register-text">Not a member? <%= link_to 'Register here', register_path %></small>